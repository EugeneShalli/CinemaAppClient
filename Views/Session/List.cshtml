@model IEnumerable<Session>

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<div class="d-flex flex-row justify-content-between mb-4">
  <span class="d-inline"><h2>Sessions</h2></span>
  @Html.ActionLink("Add Session", "Add", "Session", "",new { @class="btn btn-primary", role="button"})
</div>


<div class="container">
    
    
   <table class="table table-hover">
     <thead>
       <tr>
         <th scope="col">#</th>
         <th scope="col">Date</th>
         <th scope="col">Room</th>
         <th scope="col">Film</th>
         
       </tr>
     </thead>
     <tbody>
        @foreach (var forecast in this.Model)
         {
          <tr>
            <th scope="row">@forecast.Id</th>
            <td>@forecast.Date</td>
            <td>@forecast.Room</td>
            <td>@forecast.FilmId</td>
          </tr>
         }
     </tbody>
   </table>
</div>